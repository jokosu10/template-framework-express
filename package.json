{
  "name": "template-framework-express",
  "version": "2.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "db_drop_development": "cross-env NODE_ENV=development ./node_modules/.bin/sequelize db:drop",
    "db_create_development": "cross-env NODE_ENV=development ./node_modules/.bin/sequelize db:create",
    "table_development": "cross-env NODE_ENV=development ./node_modules/.bin/sequelize db:migrate",
    "seed_development": "cross-env NODE_ENV=development ./node_modules/.bin/sequelize db:seed:all",
    "migrate": "cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:migrate && cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:seed:all",
    "migrate:reset": "cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:migrate:undo:all && npm run migrate",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test": "cross-env NODE_ENV=test jest __tests__ --coverage controllers",
    "dev": "./node_modules/.bin/nodemon app.js"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": false
  },
  "author": "Joko Susilo <jokosu10@opensuse.org>",
  "license": "MIT License",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "header-parser": "^1.0.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}